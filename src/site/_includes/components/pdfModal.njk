<div id="pdf-viewer-modal" class="pdf-viewer-modal" aria-hidden="true" role="dialog" aria-modal="true">
  <div class="pdf-viewer-backdrop" data-pdf-close></div>
  <div class="pdf-viewer-dialog" role="document">
    <button class="pdf-viewer-close" type="button" aria-label="Close reader" data-pdf-close>×</button>
    <div class="pdf-viewer-frame-wrap">
      <div class="pdf-viewer-loading" hidden>Loading…</div>
      <iframe id="pdf-viewer-iframe" title="PDF Reader" loading="lazy"></iframe>
    </div>
  </div>
</div>


